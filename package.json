{
  "name": "echoai",
  "version": "2.3.0",
  "description": "Echo - Intelligent AI terminal with autonomous agents, 6 providers (Claude, GPT, Groq, OpenRouter, Meta, Gemini), and advanced output optimization",
  "main": "dist/index.js",
  "bin": {
    "echoai": "dist/cli.js",
    "echo-ai": "dist/cli.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/cli.js",
    "build:watch": "tsc --watch",
    "dev": "tsx src/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "clean": "rm -rf dist coverage .nyc_output",
    "prepack": "npm run clean && npm run build",
    "postinstall": "node -e \"console.log('\\nðŸ”® Echo AI CLI installed! Run: echoai\\n')\"",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:major": "npm version major && npm publish",
    "release": "npm version patch && git push --follow-tags",
    "release:minor": "npm version minor && git push --follow-tags",
    "release:major": "npm version major && git push --follow-tags"
  },
  "keywords": [
    "echo",
    "ai",
    "cli",
    "agents",
    "claude",
    "gpt",
    "gemini",
    "groq",
    "meta",
    "llama",
    "developer-tools",
    "vscode",
    "automation",
    "optimization"
  ],
  "author": {
    "name": "Vijeet Shah",
    "email": "vijeet@vijeetshah.com",
    "url": "https://vijeetshah.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vijeet-shah/echo-ai-cli.git"
  },
  "bugs": {
    "url": "https://github.com/vijeet-shah/echo-ai-cli/issues"
  },
  "homepage": "https://github.com/vijeet-shah/echo-ai-cli#readme",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.0",
    "@echoai/core": "workspace:*",
    "@echoai/gateway": "workspace:*",
    "@echoai/memory": "workspace:*",
    "@echoai/skills": "workspace:*",
    "@types/diff": "^7.0.2",
    "@types/js-yaml": "^4.0.9",
    "@types/ws": "^8.18.1",
    "axios": "^1.6.0",
    "better-sqlite3": "^12.2.0",
    "commander": "^11.1.0",
    "cosmiconfig": "^9.0.0",
    "diff": "^8.0.2",
    "dotenv": "^16.3.0",
    "eventsource": "^4.0.0",
    "groq-sdk": "^0.7.0",
    "hono": "^4.0.0",
    "inquirer": "^9.2.0",
    "js-yaml": "^4.1.0",
    "openai": "^4.20.0",
    "uuid": "^12.0.0",
    "ws": "^8.18.3",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/eventsource": "^1.1.15",
    "@types/inquirer": "^9.0.0",
    "@types/node": "^20.8.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.2.0",
    "vitest": "^1.0.0"
  }
}